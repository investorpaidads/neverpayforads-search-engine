{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/app/**/*.{ts,tsx}",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app/api/$1"
    }
  ],
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "includeFiles": "public/data/*.db,public/data/database-index.json"
    }
  }
}
